{
  "name": "@rainbow6/api",
  "version": "0.0.6",
  "publishConfig": {
    "access": "public"
  },
  "description": "Typescript SDK for the Rainbow 6 API",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json --passWithNoTests --verbose --silent false",
    "build": "tsc",
    "lint": "tsc --noEmit && eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elviswolcott/r6-api.git"
  },
  "keywords": [],
  "author": "Elvis Wolcott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elviswolcott/r6-api.git"
  },
  "homepage": "https://github.com/elviswolcott/r6-api#readme",
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.10",
    "jest": "^25.1.0",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "fetch-mock": "^9.3.1",
    "node-fetch": "^2.6.0"
  }
}
